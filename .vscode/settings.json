{
    "editor.formatOnSave": true,
    "[typescript]": {
      "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
      }
    },
    "java.saveActions.organizeImports": true,
    "[css]": {
      "editor.suggest.insertMode": "replace"
    },
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/Thumbs.db": true,
      "**/node_modules": true,
      "**/.next": true
    },
    "editor.fontLigatures": false,
    "cSpell.words": [
      "fampay"
    ]
  }
  